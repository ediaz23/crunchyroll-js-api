        "url": "https://github.com/ediaz23/crunchyroll_js_api"
    },
    "exports": {
        "development": "./dist/crunchyroll-js-api.debug.js",
        "production": "./dist/crunchyroll-js-api.min.js",
        "default": "./dist/crunchyroll-js-api.min.js",
        "types": "./dist/types/index.d.ts"
    },
    "dependencies": {},
    "devDependencies": {
        "eslint": "^8.34.0",
        "form-data": "^4.0.0",
